{
  "name": "pi-sip-alarm-server",
  "version": "1.0.0",
  "private": true,
  "main": "./src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "dotenv -e ../.env -v NODE_ENV=development tsx watch --clear-screen=false ./src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node ../build/server/src/index.js",
    "deploy": "yarn build && yarn start"
  },
  "dependencies": {
    "chokidar": "^3.5.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "get-port": "^5.1.1",
    "onoff": "^6.0.3",
    "socket.io": "^4.1.3",
    "text2wav": "^0.0.13",
    "tiny-typed-emitter": "^2.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^15.6.0",
    "@types/uuid": "^8.3.1",
    "dotenv-cli": "^7.4.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^4.2.4"
  }
}
